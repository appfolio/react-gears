{
  "name": "react-gears",
  "version": "4.25.0",
  "description": "React-based version of Gears",
  "author": "Appfolio, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/appfolio/react-gears.git"
  },
  "license": "MIT",
  "main": "dist/bundle.js",
  "files": [
    "dist",
    "lib"
  ],
  "publishConfig": {
    "registry": "https://nexus.dev.appf.io/repository/npm-hosted/"
  },
  "scripts": {
    "lint": "eslint src test",
    "start": "BABEL_ENV=storybook start-storybook -p 6006 -s ./public",
    "ts-test": "NODE_ENV=test tsc ./test/typesafety.tsx --jsx react --noEmit",
    "test": "NODE_ENV=test nyc --reporter text --reporter lcov mocha  --require ./test/helpers.js",
    "test:watch": "NODE_ENV=test nyc --reporter text --reporter lcov mocha  --watch --require ./test/helpers.js",
    "docs": "BABEL_ENV=storybook build-storybook -o docs",
    "version": "npm run docs && git add -A docs",
    "dist:copy-d-ts-files": "node ./scripts/copy_d_ts_files.js",
    "dist": "npm run dist:copy-d-ts-files && webpack -p",
    "prepublishOnly": "npm run dist",
    "postpublish": "git push --no-verify && git push --tags"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "dependencies": {
    "@types/reactstrap": "6.0.2",
    "classnames": "^2.2.6",
    "credit-card-type": "^5.0.1",
    "date-fns": "^1.30.1",
    "enzyme-adapter-react-16": "^1.13.0",
    "fecha": "^2.3.3",
    "lodash.flow": "^3.5.0",
    "lodash.includes": "^4.3.0",
    "lodash.isequal": "^4.5.0",
    "lodash.noop": "^3.0.1",
    "lodash.orderby": "^4.6.0",
    "lodash.range": "^3.2.0",
    "lodash.some": "^4.6.0",
    "lodash.tolower": "^4.1.2",
    "lodash.topairs": "^4.3.0",
    "lodash.without": "^4.4.0",
    "memoize-one": "^5.0.4",
    "prop-types": "^15.7.2",
    "react-fontawesome": "^1.4.0",
    "react-onclickoutside": "^6.8.0",
    "react-popper": "^0.10.4",
    "react-resize-detector": "^4.1.4",
    "react-select-plus": "1.2.0",
    "react-sortable-hoc": "^1.9.1",
    "react-text-mask": "~5.0.2",
    "react-transition-group": "^2.9.0",
    "reactstrap": "~6.3.1",
    "text-mask-addons": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@storybook/addon-info": "^3.4.12",
    "@storybook/addon-knobs": "^3.4.12",
    "@storybook/addon-options": "^3.4.12",
    "@storybook/addon-viewport": "^3.4.12",
    "@storybook/react": "^3.4.12",
    "@types/react": "16.4.1",
    "assert": "~1.4.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "css-loader": "^0.28.11",
    "enzyme": "^3.9.0",
    "eslint": "^4.19.1",
    "eslint-config-appfolio-react": "^1.1.0",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "fs-extra": "5.0.0",
    "glob": "^7.1.4",
    "ignore-styles": "~5.0.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha": "~3.5.3",
    "node-sass": "^4.12.0",
    "nyc": "^12.0.2",
    "postcss": "^7.0.16",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^6.0.7",
    "sinon": "~3.2.1",
    "style-loader": "~0.18.1",
    "ts-node": "^5.0.0",
    "typescript": "2.9.1",
    "uncontrollable": "^4.1.0",
    "webpack": "^4.31.0"
  }
}
