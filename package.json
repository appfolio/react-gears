    "uncontrollable": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "browserslist": "last 2 versions",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>test/helpers.js"
    ]
  }
